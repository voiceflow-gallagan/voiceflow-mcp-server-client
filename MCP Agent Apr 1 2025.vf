  "functions": [
    {
      "id": "67e3dc6ce5a8e42a46e26bc2",
      "name": "MCP Tools (copy)",
      "createdByID": 3600,
      "folderID": null,
      "code": "export default async function main(args) {\n\n  const { inputVars } = args\n\n  for (const key in inputVars) {\n    if (inputVars.hasOwnProperty(key)) {\n      inputVars[key] = inputVars[key].trim();\n    }\n  }\n\n  let nextPath = 'default'\n  const outputVars = {}\n  const trace = []\n\n  outputVars.answer = null\n  outputVars.error = ''\n  outputVars.toolsResponses = null\n  //outputVars.conversationId = inputVars.conversationId || '0'\n\n  try {\n\n      const body = {\n        query: inputVars.query,\n        llm_answer: false,\n        userId: inputVars.userId || 'none',\n        queryTimeoutMs: 120000\n      };\n      \n      if (inputVars.conversationId && inputVars.conversationId !== '0') {\n        body.conversationId = inputVars.conversationId;\n      }\n\n      const messageResponse = await fetch(`https://mcp-client.proxy-voiceflow.com/api/query`, {\n        method: 'POST',\n        headers: {\n            'Content-Type': 'application/json'\n        },\n        body: JSON.stringify(body)\n      })\n      if (!messageResponse.ok) throw new Error(`MCP Tools Error (Message)`)\n\n    \n\n    const message = await messageResponse.json\n\n    outputVars.answer = message.answer || ''\n    outputVars.toolsResponses = JSON.stringify(message.toolResponses) || ''\n\n    /* trace.push({\n      type: 'debug',\n      payload: {\n        message: 'All good'\n      }\n    }) */\n\n  } catch(error) {\n    nextPath = 'error'\n    outputVars.error = error.message\n  }\n\n  return {\n    next: { path: nextPath },\n    outputVars: outputVars,\n    trace: trace\n  }\n}",
      "image": "https://cm4-production-assets.s3.amazonaws.com/1715717616588-openai-logo-1.png",
      "description": "This function will use MCP tools to help users with their requests and return an answer.",
